<div class="document-contextual-dropdown">
  <p class="dropdown-save-button" style="display:none;"       ><span class="icon is-small"><i class="fa fa-fw fa-save"></i></span> &nbsp; Save Document</p>
  <p onclick="showExportDocModal();"                          ><span class="icon is-small"><i class="fa fa-fw fa-share-square-o"></i></span> &nbsp; Export Document</p>
  <p onclick="showRenameDocModal();" class="is-hidden-offline"><span class="icon is-small"><i class="fa fa-fw fa-i-cursor"></i></span> &nbsp; Rename Document</p>
  <p onclick="closeDoc();" class="is-hidden-offline"          ><span class="icon is-small"><i class="fa fa-fw fa-close"></i></span> &nbsp; Close Document</p>
  <p onclick="showDeleteDocModal();"                          ><span class="icon is-small"><i class="fa fa-fw fa-trash"></i></span> &nbsp; Delete Document</p>
</div>
