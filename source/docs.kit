<body class="docs-body">
<div id="docs-main-wrap">
  <progress id="main-progress" class="no-print progress is-small is-success" value="100" max="100"></progress>

  <div id="docs-left-wrap" class="no-print is-loading">
    <div id="docs-left-top" class="firstLoad">
      <a class="docs-nav-item rememberKey top-left-icon" href="home"><img src="../assets/cryptee-logo-b.svg" alt="Cryptee | Dashboard"></a>
      <a class="docs-nav-item is-hidden-offline clickable tag is-success" id="upgrade-badge">UPGRADE</a>
    </div>

    <div id="docs-left-center" class="left-view-controller-buttons firstLoad">
      <!-- @import "docs-search.kit" -->
      <div id="offline-button" class="offlineLeftButtons left-nav-buttons is-unavailable" pos="2">
        <span class="button is-light left-button">
          <span class="fa-stack fa-lg">
            <i class="fa fa-cloud fa-stack-1x" style=""></i>
            <i class="fa fa-times fa-stack-1x text-danger" style="color: #FFF; font-size: 8px !important;"></i>
          </span>
        </span>
        <span class="left-nav-field">Offline</span>
      </div>

      <div id="recents-button" class="onlineLeftButtons left-nav-buttons active" pos="0">
        <span class="button is-light left-button">
          <i class="fa fa-fw fa-clock-o"></i>
        </span>
        <span class="left-nav-field">Recent Documents</span>
      </div>

      <div id="folders-button" class="onlineLeftButtons left-nav-buttons" pos="1">
        <span class="button is-light left-button">
          <i class="fa fa-fw fa-folder"></i>
        </span>
        <span class="left-nav-field">All Files &amp; Folders</span>
      </div>

      <div id="homedoc" class="onlineLeftButtons left-nav-buttons">
        <span class="button is-light left-button">
          <i class="fa fa-fw fa-home"></i>
        </span>
        <span class="left-nav-field">Home Document</span>
      </div>

    </div>

    <div id="docs-left-bottom" class="firstLoad">

      <div id="results" style="display:none;">
        <div class="notification">
          <p>Test Note</p>
        </div>
      </div>

      <div class="left-views-container">
        <div class="left-views-scroller pos-0">
          <div class="left-view">
            <!-- @import "docs-recent.kit" -->
          </div>
          <div class="left-view">
            <!-- @import "docs-folders.kit" -->
          </div>

          <div class="left-view">
            <!-- @import "docs-offline.kit" -->
          </div>
        </div>
      </div>

    </div>

  </div>

  <div id="docs-center-wrap" class="is-loading">
    <!-- TOOLS -->

    <!-- @import "docs-no-offline-docs.kit" -->
    <!-- @import "docs-outdated-messages.kit" -->

    <div id="editor-toolbar" class="no-print">
      <!-- @import "docs-toolbar.kit" -->
    </div>

    <span id="close-menu-button" class="icon no-print"> <i class="fa fa-chevron-left" aria-hidden="true"></i> </span>

    <div id="mobile-topbar" class="no-print">
      <!-- @import "docs-mobile-topbar.kit" -->
    </div>

    <div id="fileLoadingStatus" class="no-print">
      <div class="message">
        Starting Up<br>
        <span class='cancel-loading'>One Moment Please</span>
      </div>
    </div>

    <div id="docs-page-wrap">
      <!-- QUILL EDITOR INJECTED HERE -->
    </div>
  </div>

  <div id="docs-right-wrap" class="no-print is-loading">
    <!-- @import "docs-sections.kit" -->
  </div>
</div>

<span class="icon no-print clickable" id="help-button" onclick="showModal('help-modal')" title="Help"><i class="fa fa-fw fa-question"></i></span>
<span class="icon no-print clickable" id="hotkeys-button" onclick="toggleHotkeys();" title="Markdown & Shortcuts"><i class="fa fa-keyboard-o"></i></span>

