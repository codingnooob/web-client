<script type="text/javascript">
$('.period-button').on('click', function(event) {
  $("#priceToPay").html($(this).attr("amount"));
  $(".additional-price").html($(this).attr("additional"));
  $(".per-period").html(" / " + $(this).attr("period"));
  $(".period-button").addClass('is-white');
  $(this).removeClass("is-white");
});

var slideCounter = 0;
var totalSlides = 3;
window.setInterval(function(){
  slideCounter++;
  if (slideCounter > totalSlides) {
    slideCounter = 0;
    $("#hero").removeClass('hero'+(totalSlides)).addClass('hero'+slideCounter);
  } else {
    $("#hero").removeClass('hero'+(slideCounter-1)).addClass('hero'+slideCounter);
  }
}, 4250);

</script>
